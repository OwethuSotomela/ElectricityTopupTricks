<div class="container">
    <form action="/use_electricity" method="GET">
        <label for="appliances">Choose an Appliance:</label>
        <select name="cars" id="cars">
            {{#each appliances}}
            <option value="{{name}}">{{name}}</option>
            {{/each}}
        </select>
        <br><br>
        <input type="submit" value="Submit">
    </form>
</div>